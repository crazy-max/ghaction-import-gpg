{
  "name": "import-gpg",
  "description": "GitHub Action to easily import a GPG key",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest --coverage",
    "pre-checkin": "yarn run format && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazy-max/ghaction-import-gpg.git"
  },
  "keywords": [
    "actions",
    "gpg",
    "signing",
    "git"
  ],
  "author": "CrazyMax",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/exec": "^1.0.4",
    "addressparser": "^1.0.1",
    "openpgp": "^4.10.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/openpgp": "^4.4.12",
    "@vercel/ncc": "^0.23.0",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "jest-runtime": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "typescript-formatter": "^7.2.2"
  }
}
